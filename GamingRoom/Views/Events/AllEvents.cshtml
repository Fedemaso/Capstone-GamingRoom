@model List<GamingRoom.Models.Events>

<hr />

<div class="container-fluid mt-4" >
    <div class="row" >
        @foreach (var evento in Model)
        {
            <div class="col-xxl-2 col-xl-3 col-lg-4 col-md-6 col-sm-12 mb-4 fs-5">
                @Html.Partial("_AllEventCard", evento)
            </div>
        }
    </div>
</div>
